---
interface Props {
	href?: string;
}

const { href } = Astro.props;
const videoSrc = `https://www.youtube.com/embed/${href}`;
---
<div class="flex w-1/3 flex-wrap">
    <div class="w-full p-1 md:p-2">
        <div class="rounded-full">

            <iframe class=""  src={videoSrc}  allowfullscreen></iframe>
        </div>

    </div>
  </div>